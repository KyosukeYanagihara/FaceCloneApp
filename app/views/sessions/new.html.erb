<div class="color-white margin-top">
<h2>ログイン</h2>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.submit "ログイン", class: "btn btn-" %>
<% end %>
</div>